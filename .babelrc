{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@/src": "./src",
          "@/pages": "./pages",
          "@/public": "./public",
          "@/request": "./src/utils/request",
          "@/components": "./src/components"
        }
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}
